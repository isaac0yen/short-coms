{
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "env": {
      "commonjs": true,
      "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
      "semi": ["warn", "always"],
      "quotes": ["error", "double"],
      "indent": ["error", 2],
      "no-var": "error",
      "prefer-const": "error"
  }
}